<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/app_gradient"
    tools:context=".Organizerscreen">

    <Button
        android:id="@+id/btnproceed"
        android:layout_width="209dp"
        android:layout_height="49dp"
        android:layout_marginTop="24dp"
        android:text="Proceed"
        app:layout_constraintEnd_toEndOf="@+id/elegAge"
        app:layout_constraintHorizontal_bias="0.587"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elegAge" />

    <EditText
        android:id="@+id/orgName"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="@font/roboto_regular"
        android:hint="Name"
        android:inputType="textPersonName"
        android:padding="15dp"
        android:textColor="#3C3A3A"
        android:textColorHint="#605F5F"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="355dp"
        android:layout_height="68dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:fontFamily="@font/roboto_regular"
        android:padding="15dp"
        android:text="Eligibility"
        android:textColor="#3C3A3A"
        android:textColorHint="#605F5F"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgVenue" />

    <EditText
        android:id="@+id/orgEdate"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="@font/roboto_regular"
        android:hint="End Date"
        android:inputType="textPersonName"
        android:padding="15dp"
        android:textColor="#3C3A3A"
        android:textColorHint="#605F5F"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgSdate" />

    <EditText
        android:id="@+id/orgVenue"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="@font/roboto_regular"
        android:hint="venue"
        android:inputType="textPersonName"
        android:padding="15dp"
        android:textColor="#3C3A3A"
        android:textColorHint="#605F5F"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgEdate" />

    <EditText
        android:id="@+id/eligDegree"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="@font/roboto_regular"
        android:hint="Degree"
        android:inputType="textPersonName"
        android:padding="15dp"
        android:textColor="#3C3A3A"
        android:textColorHint="#605F5F"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <EditText
        android:id="@+id/elegAge"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="@font/roboto_regular"
        android:hint="Age"
        android:inputType="textPersonName"
        android:padding="15dp"
        android:textColor="#3C3A3A"
        android:textColorHint="#605F5F"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elegsem" />

    <EditText
        android:id="@+id/elegsem"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="@font/roboto_regular"
        android:hint="semester"
        android:inputType="textPersonName"
        android:padding="15dp"
        android:textColor="#3C3A3A"
        android:textColorHint="#605F5F"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eligDegree" />

    <EditText
        android:id="@+id/orgSdate"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:fontFamily="@font/roboto_regular"
        android:hint="Start Date"
        android:inputType="textPersonName"
        android:padding="15dp"
        android:textColor="#3C3A3A"
        android:textColorHint="#605F5F"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgName" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="112dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        android:text="Add Event"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnproceed" />

</androidx.constraintlayout.widget.ConstraintLayout>